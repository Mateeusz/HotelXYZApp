                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                     </VBox.margin>
                  </PasswordField>
                  <TextField fx:id="nameLabel" prefHeight="20.0" prefWidth="100.0" promptText="Imię">
